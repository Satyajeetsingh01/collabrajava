//MAP
import java.util.HashMap;
public class Maps
{
	public static void main(String[] args) 
	{
		HashMap map=new HashMap();
		map.put(1,900);
		map.put(2,"hello");
		map.put('b',"map");
		map.put(null,null);
		map.put(3,null);
		map.put("*","500");
		System.out.println(map);

		//only keys
		Set s=map.keySet();
		System.out.println(s);

		//only values
		Collection c= map.values();
		System.out.println(c);
	}
}

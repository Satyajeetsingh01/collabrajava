//Question 8 find factorial of even digits of a given number
class factoriale
{
	public static void main(String[] args) 
	{
		int num=32145;
		while(num>0)
		{
			int temp=num%10;
			
			if(temp%2==0)
			{
				int fact=1;
				while(temp>0)
				{
					fact=fact*temp;
					temp--;
				}
				System.out.println(fact);
			}
			num=num/10;	
		}
	}
}
